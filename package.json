{
  "name": "nintendo-games",
  "version": "0.0.0",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/scss/ -o static",
    "build-task:autoprefixer": "postcss static/*.css --use autoprefixer -d static",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/scss/**/*.scss' -c 'npm run sass:build'",
    "serve": "stacy serve",
    "dev": "npm-run-all -p sass:* serve",
    "build": "stacy generate"
  },
  "devDependencies": {
    "stacy": "^2.0.0"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "private": true,
  "dependencies": {
    "autoprefixer": "^10.3.0",
    "chokidar-cli": "^2.1.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1"
  }
}
